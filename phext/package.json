{
  "name": "phext",
  "displayName": "phext",
  "description": "This extension allows you to interact with a sparse 9D matrix of scrolls. Welcome to the Exocortex\nSee https://www.phext.io/ for more information.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.100.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "browser": "./dist/web/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "phext",
        "extensions": [
          ".phext"
        ],
        "aliases": [
          "Phext"
        ]
      }
    ],
    "customEditors": [
      {
        "viewType": "phext.editor",
        "displayName": "Phext Editor",
        "selector": [
          {
            "filenamePattern": "*.phext"
          }
        ]
      }
    ]
  },
  "scripts": {
    "test": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=. --extensionTestsPath=dist/web/test/suite/extensionTests.js",
    "pretest": "npm run compile-web",
    "vscode:prepublish": "npm run package-web",
    "compile-web": "npm run check-types && npm run lint && node esbuild.js",
    "watch-web": "npm-run-all -p watch-web:*",
    "watch-web:esbuild": "node esbuild.js --watch",
    "watch-web:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package-web": "npm run check-types && npm run lint && node esbuild.js --production",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "run-in-browser": "vscode-test-web --browserType=chromium --extensionDevelopmentPath=. ."
  },
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@types/assert": "^1.5.11",
    "@types/mocha": "^10.0.10",
    "@types/vscode": "^1.100.0",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vscode/test-web": "^0.0.69",
    "assert": "^2.1.0",
    "esbuild": "^0.25.3",
    "eslint": "^9.25.1",
    "glob": "^11.0.2",
    "mocha": "^11.1.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "buffer": "^6.0.3"
  }
}
